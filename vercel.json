{
    "redirects": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "www.oryn.tw"
                }
            ],
            "destination": "https://oryn.tw/:path*",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/((?!sitemap.xml|robots.txt|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}